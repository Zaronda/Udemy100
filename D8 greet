# call greet function
#greet()

def greet_with(name, location):
    print(f"Hello {name}")
    print(f"What is it like in {location}?")

# positional arguments
greet_with("Eleanor", "Bedroom")

# keyword arguments
greet_with(location="Home", name="Alaca")
